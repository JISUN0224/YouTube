{
  "hosting": [
    {
      "site": "myprojectweb-yt",
      "public": "Youtube/dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
